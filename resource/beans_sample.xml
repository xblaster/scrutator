<?xml version="1.0" encoding="UTF-8"?>
<beans>
	<bean id="servicesObj" class="scrutator.core.server.SCRTServices"/>
	<bean id="distantConnect" class="scrutator.core.server.SCRTXMLRPC">
		<constructor-arg>
			<ref bean="servicesObj"/>
			<value>7080</value>
		</constructor-arg>
	</bean>
	
    <bean id="eventManager" class="scrutator.core.manager.SimpleEvent"/>
	<bean id="eventManager2" class="scrutator.core.manager.SimpleEvent">
		<constructor-arg>
			<value>12</value>
			<ref bean="eventManager"/>
		</constructor-arg>
		
        <property name="companyName">
            <value>lizjason</value>
        </property>
		 <property name="event">
			<ref bean="eventManager"/>
        </property>

    </bean>
</beans>